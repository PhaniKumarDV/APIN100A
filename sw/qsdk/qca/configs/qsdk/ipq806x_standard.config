CONFIG_TARGET_ipq806x=y
CONFIG_TARGET_ipq806x_QSDK_Standard=y
CONFIG_TARGET_BOARD="ipq806x"
CONFIG_COLLECT_KERNEL_DEBUG=y
CONFIG_KERNEL_DYNAMIC_DEBUG=y
