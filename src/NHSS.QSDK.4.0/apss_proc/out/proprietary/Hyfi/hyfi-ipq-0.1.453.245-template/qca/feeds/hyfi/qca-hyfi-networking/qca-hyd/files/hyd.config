config Topology 'Topology'
	option ND_UPDATE_INTERVAL '15'
	option BD_UPDATE_INTERVAL '3'
	option HOLDING_TIME '190'
	option TIMER_LOW_BOUND '7'
	option TIMER_UPPER_BOUND '11'
